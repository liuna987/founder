<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
 
 <package name="uu" namespace="/emp" extends="struts-default">
 
  <action name="EmpAction" class="zhu.action.EmpAction">
  <result name="login"  type="redirectAction">
  <param name="actionName">EmpAction</param>
   <param name="Method">listfindAll</param>
  </result>
   <result name="fail">/jsp/fail.jsp</result>
   <result name="findAll">/jsp/emp.jsp</result>
    <result name="update">/jsp/update.jsp</result>
     <result name="insert">/jsp/save.jsp</result>
 </action> 
 
 </package>
</struts>    
